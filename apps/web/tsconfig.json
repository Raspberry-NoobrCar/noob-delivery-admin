{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "baseUrl": ".",
    "paths": {
      "@/components": ["app/_components"],
      "@/services": ["app/_services"],
      "@/interfaces": ["app/_interfaces"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/_services/fetcher.ts"],
  "exclude": ["node_modules"]
}
